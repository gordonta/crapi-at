{
  "version": "1.1.0",
  "applicationId": "ee5cc239-3b64-4045-ba71-ff40c03a35fd",
  "host": "http://host.docker.internal",
  "sources": [
    {
      "name": "crapi",
      "openApi": {
        "sourcePath": "/openapi-spec/crapi-spec.json"
      }
    }
  ],
  "auths": [
    {
      "name": "JaneDoe",
      "role": "reader / writer",
      "mappings": [
        {
          "type": "header",
          "value": "Bearer {{JaneDoe}}",
          "key": "Authorization"
        }
      ]
    }
  ],
  "app": {
    "secrets": [
      {
        "name": "JaneDoe",
        "type": "postman",
        "data": {
          "sourceBody": "$.token"
        }
      }
    ]
  }
}